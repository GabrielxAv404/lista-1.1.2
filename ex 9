#include <stdio.h>
#include <string.h>


int main() {
    char nome[10][100];
    char busca[100];
    int i, j;

    
    printf("Digite os nomes dos 10 alunos:\n");
    for(i = 0; i < 10; i++) {
        printf("Aluno %d: ", i + 1);
        fgets(nome[i], 100, stdin);

       
        nome[i][strcspn(nome[i], "\n")] = '\0';
    }

   
    char temp[100];
    for(i = 0; i < 10 - 1; i++) {
        for(j = i + 1; j < 10; j++) {
            if(strcmp(nome[i], nome[j]) < 0) {
                strcpy(temp, nome[i]);
                strcpy(nome[i], nome[j]);
                strcpy(nome[j], temp);
            }
        }
    }

    
    printf("\nNomes em ordem decrescente:\n");
    for(i = 0; i < 10; i++) {
        printf("%s\n", nome[i]);
    }

   
    printf("\nBuscar nome: ");
    fgets(busca, 100, stdin);
    busca[strcspn(busca, "\n")] = '\0'; 

    int encontrado = 0;
    for(i = 0; i < 10; i++) {
        if(strcmp(nome[i], busca) == 0) {
            printf("Nome '%s' encontrado na posicao %d\n", busca, i);
            encontrado = 1;
            break;
        }
    }

    if(!encontrado) {
        printf("Nome '%s' nao encontrado\n", busca);
    }

    return 0;
}